# Arquivo de exemplo de credenciais para configuração LocalStack Cognito
# Copie este arquivo para credentials.tf e atualize com seus valores
# credentials.tf está no .gitignore e não deve ser commitado

variable "admin_users" {
  description = "Lista de usuários administradores para criar no Cognito"
  type = list(object({
    email = string
    name  = string
  }))
  default = [
    {
      email = "admin@example.com"
      name  = "Usuário Administrador"
    }
  ]
}

variable "reviewer_users" {
  description = "Lista de usuários revisores para criar no Cognito"
  type = list(object({
    email = string
    name  = string
  }))
  default = [
    {
      email = "reviewer@example.com"
      name  = "Usuário Revisor"
    }
  ]
}

variable "user_cognito" {
  description = "Configuração do usuário principal para Cognito"
  type        = map(string)
  default = {
    email = "user@example.com"
    name  = "Usuário Padrão"
  }
}
